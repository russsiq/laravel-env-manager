{
    "name": "russsiq/laravel-env-manager",
    "description": "Менеджер файла переменных окружения Laravel 7.x.",
    "keywords": ["laravel", "environment file", "manager"],
    "homepage": "https://github.com/russsiq/laravel-env-manager",
    "type": "library",

    "license": "MIT",
    "authors": [{
        "name": "Рустам Гимранов",
        "email": "rustam_gimranov@mail.ru",
        "homepage": "https://github.com/russsiq",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "8.*"
    },
    "require-dev": {
        "illuminate/encryption": "8.*",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Russsiq\\EnvManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Russsiq\\EnvManager\\EnvManagerServiceProvider"
            ],
            "aliases": {
                "EnvManager": "Russsiq\\EnvManager\\Facades\\EnvManager"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-win7": "vendor\\bin\\phpunit",
        "testdox": "vendor\\bin\\phpunit --testdox-html tests/testdox.html"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
